<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Model.Booking" table="Ticket">
		<id name="id2" column="id2" >
			<generator class="native"/>
		</id>
		
		<property name="username" length="20"/>
		<property name="sex" length="2"/>
		<property name="origin" length="20"/>
		<property name="destn" length="20"/>
		<property name="date"/>
		<property name="id" length="20"/>
	</class>
</hibernate-mapping>