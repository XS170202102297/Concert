<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.system.management.model.Booking" table="Users">
		<id name="id2" column="id2" >
			<generator class="native"/>
		</id>
		
		<property name="username" length="20"/>
		<property name="sex" length="2"/>
		<property name="mobilePhone" length="20"/>
		<property name="age" />
		<property name="password"/>
		<property name="email" length="20"/>
	</class>
</hibernate-mapping>